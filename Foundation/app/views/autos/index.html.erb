<p id="notice"><%= notice %></p>

<h1>Autos</h1>

<table>
  <thead>
    <tr>
      <th>Marca</th>
      <th>Modelo</th>
      <th>Anio</th>
      <th>Precio</th>
      <th>Fecha adquisicion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autos.each do |auto| %>
      <tr>
        <td><%= auto.marca %></td>
        <td><%= auto.modelo %></td>
        <td><%= auto.anio %></td>
        <td><%= auto.precio %></td>
        <td><%= auto.fecha_adquisicion %></td>
        <td><%= link_to 'Show', auto, class:'button radius' %></td>
        <td><%= link_to 'Edit', edit_auto_path(auto), class:'button radius success' %></td>
        <td><%= link_to 'Destroy', auto, method: :delete, data: { confirm: 'Are you sure?' }, class:'button radius alert'%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Auto', new_auto_path, class:'button round success' %>
